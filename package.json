{
  "name": "get-shop-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate --config ormconfig.ts --connection  --name ",
    "migration:create": "npm run typeorm -- migration:create --config ormconfig.ts -n",
    "migration:up": "npm run typeorm -- migration:run --config ormconfig.ts",
    "migration:down": "npm run typeorm -- migration:revert --config ormconfig.ts",
    "seed:create": "npm run typeorm -- migration:create --config ormconfig.seeder.ts -n",
    "seed:up": "npm run typeorm -- migration:run --config ormconfig.seeder.ts",
    "seed:down": "npm run typeorm -- migration:revert --config ormconfig.seeder.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "postinstall": "npm run prepare",
    "docker:services": "docker-compose -f ./containers/docker-compose.services.yml --env-file=.env",
    "docker:services:up": "npm run docker:services -- up",
    "docker:services:down": "npm run docker:services -- down",
    "docker": "npm run docker:services -- -f ./containers/docker-compose.base.yml ",
    "docker:dev": "npm run docker -- -f ./containers/docker-compose.dev.yml",
    "docker:dev:up": "npm run docker:dev -- up",
    "docker:dev:down": "npm run docker:dev -- down"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express-session": "^1.17.2",
    "nodemailer": "^6.7.3",
    "openid-client": "^5.1.4",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@getshop/arca-ipay(|/.*)$": "<rootDir>/libs/arca-ipay/src/$1"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
